{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean-all": "rm -rf node_modules/ .next/ public/ scripts/ src/ && git status",
    "clean-revert": "git checkout public/ scripts/ src/",
    "clean-start": "pnpm i && pnpm build && pnpm dev",
    "copy-finish": "git checkout src/app/lib/interfaces.ts src/app/blog/post/[id]/error.tsx  src/app/blog/post/[id]/loading.tsx && git diff src/app/lib/data.ts src/app/layout.tsx && git status && rm package-lock.json",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node -r dotenv/config ./scripts/seed.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "1.0.2",
    "@next/env": "^16.1.6",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "beta",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "9.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  }
}
